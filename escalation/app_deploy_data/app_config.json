{
    "title": "Yeast States Escalation",
    "brief_desc": "Demo for the Yeast States Escalation OS",
    "data_backend": "psql",
    "data_sources": [
        "flow_meta",
        "flow_stat_wide",
        "growth_rate",
        "plate_reader"
    ],
    "available_pages": {
        "replicate_summary": {
            "button_label": "Data Converge Replicate Summary",
            "graphics": {
                "plate_reader_circuit_function": {
                    "plot_manager": "plotly",
                    "data_sources": [
                        {
                            "data_source_type": "plate_reader"
                        }
                    ],
                    "title": "Circuit function from plate reader",
                    "brief_desc": "",
                    "data": {
                        "points_0": {
                            "y": "plate_reader:fluor_gain_0.16/od",
                            "x": "plate_reader:replicate_group"
                        }
                    },
                    "data_filters": [
                        {
                            "type": "filter",
                            "column": "plate_reader:experiment_reference",
                            "selected": [
                                "YeastSTATES-Beta-Estradiol-OR-Gate-Plant-TF-Dose-Response"
                            ]
                        }
                    ],
                    "plot_specific_info": {
                        "data": [
                            {
                                "type": "scatter",
                                "mode": "markers"
                            }
                        ],
                        "layout": {
                            "hovermode": "closest"
                        }
                    },
                    "visualization_options": [
                        {
                            "type": "hover_data",
                            "column": [
                                "plate_reader:replicate_group_string",
                                "plate_reader:experiment_reference"
                            ]
                        },
                        {
                            "type": "groupby",
                            "column": [
                                "plate_reader:replicate_group"
                            ]
                        }
                    ],
                    "selectable_data_list": [
                        {
                            "type": "select",
                            "column": "plate_reader:experiment_reference",
                            "options": {
                                "multiple": true
                            }
                        },
                        {
                            "type": "select",
                            "column": "plate_reader:control_type",
                            "options": {
                                "multiple": true
                            }
                        },
                        {
                            "type": "select",
                            "column": "plate_reader:strain",
                            "options": {
                                "multiple": true
                            }
                        },
                        {
                            "type": "axis",
                            "column": "y",
                            "options": {
                                "entries": [
                                    "plate_reader:fluor_gain_0.10/od",
                                    "plate_reader:fluor_gain_0.16/od",
                                    "plate_reader:fluor_gain_0.20/od"
                                ]
                            }
                        }
                    ]
                }
            }
        },
